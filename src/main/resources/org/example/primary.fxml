<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="550.0"
      xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.example.Controller">
   <padding>
      <Insets bottom="1.0" left="5.0" right="5.0"/>
   </padding>
   <HBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" nodeOrientation="LEFT_TO_RIGHT"
         spacing="10.0">
      <VBox.margin>
         <Insets bottom="10.0" top="15.0"/>
      </VBox.margin>
      <Button id="linkToRocketLeague" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
              onAction="#initializeApp" text="Link to Rocket League">
         <HBox.margin>
            <Insets/>
         </HBox.margin>
      </Button>
      <Button id="rocketLeagueFileChooser" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
              onAction="#rocketLeagueDirectoryChooser" text="Choose map directory">
         <HBox.margin>
            <Insets/>
         </HBox.margin>
      </Button>
   </HBox>
   <ListView fx:id="listView" prefHeight="140.0">
      <VBox.margin>
         <Insets left="10.0" right="10.0"/>
      </VBox.margin>
   </ListView>
   <HBox alignment="CENTER" layoutX="30.0" layoutY="55.0" spacing="10.0">
      <VBox.margin>
         <Insets top="10.0"/>
      </VBox.margin>
      <Button id="injectMapButton" alignment="CENTER_RIGHT" contentDisplay="CENTER" minHeight="-Infinity"
              minWidth="-Infinity" mnemonicParsing="false" onAction="#injectMap" text="Inject"
              textAlignment="CENTER">
         <HBox.margin>
            <Insets/>
         </HBox.margin>
         <padding>
            <Insets bottom="5.0" left="20.0" right="20.0" top="5.0"/>
         </padding>
      </Button>
      <Button mnemonicParsing="false" onAction="#reverseMap" id="reverseMapButton" text="Restore"
              textAlignment="CENTER">
         <padding>
            <Insets bottom="5.0" left="20.0" right="20.0" top="5.0"/>
         </padding>
      </Button>
   </HBox>
   <HBox alignment="CENTER_RIGHT" prefHeight="11.0" prefWidth="490.0">
      <VBox.margin>
         <Insets/>
      </VBox.margin>
      <Label fx:id="linkRocketLeagueLabel" alignment="CENTER" contentDisplay="CENTER" text="Not linked !"
             textAlignment="CENTER" textFill="RED"/>
   </HBox>
</VBox>
